      <DIAGRAM FORMAT="1.00" ID="3" NAME="binary_to_decimal" CREATED="2024.03.19 13:01:40" MODIFIED="2024.03.19 13:01:40">
        <LAYOUT FORMAT="1.00" COLUMNS="2" ROWS="13">
          <ENTRIES>
